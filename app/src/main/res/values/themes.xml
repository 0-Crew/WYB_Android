<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WYB_Android" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/orange</item>
        <item name="colorPrimaryVariant">@color/orange</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/orange</item>
        <item name="colorSecondaryVariant">@color/orange</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="TextAppearance.WYBComponents" parent="android:TextAppearance">
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textColorHint">?android:textColorHint</item>
        <item name="android:textColorHighlight">?android:textColorHighlight</item>
        <item name="android:textColorLink">?android:textColorLink</item>
    </style>

    <style name="TextAppearance.WYBComponents.Bold.30.Eng" parent="TextAppearance.WYBComponents">
        <item name="fontFamily">@font/futura_bold</item>
        <item name="android:fontFamily">@font/futura_bold</item>
        <item name="android:textSize">30dp</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:includeFontPadding">false</item>
    </style>

